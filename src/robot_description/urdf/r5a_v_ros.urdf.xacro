      <controller_manager_name>controller_manager</controller_manager_name>
      <use_sim_time>true</use_sim_time>
    </plugin>
  </gazebo>
  
  <gazebo>
    <plugin name="gazebo_ros_joint_pose_trajectory" filename="libgazebo_ros_joint_pose_trajectory.so">
